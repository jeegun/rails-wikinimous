<div class="container">
  <form>
    <div class="from-group">
      <%= form_for(article) do |f| %>
        <%= f.label :title %>
        <br>
        <%= f.text_area :title, size: '50x1' %>
        <br>
        <%= f.label :content %>
        <br>
        <%= f.text_area :content, size: '50x10' %>
        <br>
        <%= f.submit %>
      <% end %>
    </div>
  </form>
</div>
